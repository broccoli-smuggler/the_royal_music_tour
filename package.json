{
  "name": "barber-jekyll",
  "description": "Barber is a blog theme for Jekyll built by Thomas Vaeth at Samesies using HTML, Sass, and JavaScript.",
  "demo": "http://barber.samesies.io",
  "version": "1.0.0",
  "scripts": {
    "start": "bundle exec jekyll serve",
    "build": "bundle exec jekyll build",
    "compile": "gulp build && bundle exec jekyll build",
    "deploy": "gulp build && bundle exec jekyll build && aws s3 sync _site/ s3://royalmusic.live --delete",
    "gulp": "gulp"
  },
  "author": "Samesies <okay@samesies.io> (http://samesies.io)",
  "keywords": [
    "barber",
    "jekyll",
    "blog"
  ],
  "dependencies": {
    "aos": "^2.2.0",
    "gulp-util": "^3.0.8",
    "jquery": "2.2.4",
    "salvattore": "^1.0.9",
    "vinyl": "^2.2.0",
    "vinyl-fs": "^3.0.3"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.2",
    "babel-preset-env": "^1.6.1",
    "babelify": "^8.0.0",
    "browserify": "^14.5.0",
    "eslint": "^4.11.0",
    "eslint-config-eslint": "^4.0.0",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^4.0.0",
    "gulp-clean-css": "^3.9.0",
    "gulp-eslint": "^4.0.0",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^3.1.0",
    "gulp-stylelint": "^5.0.0",
    "gulp-uglify": "^3.0.0",
    "gulp-zip": "^4.0.0",
    "s3-website": "^3.3.0",
    "stylelint-config-standard": "^17.0.0",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^2.0.0"
  },
  "main": "gulpfile.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joshua-ns-jordan/barber-jekyll.git"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/joshua-ns-jordan/barber-jekyll/issues"
  },
  "homepage": "https://github.com/joshua-ns-jordan/barber-jekyll#readme"
}
